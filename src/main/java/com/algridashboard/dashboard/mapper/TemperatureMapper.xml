<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.algridashboard.dashboard.mapper.TemperatureMapper" >
  <resultMap id="BaseResultMap" type="com.algridashboard.dashboard.model.Temperature" >
    <result column="temp" property="temp" jdbcType="VARCHAR" />
    <result column="time" property="time" jdbcType="VARCHAR" />
  </resultMap>
  <insert id="insert" parameterType="com.algridashboard.dashboard.model.Temperature" >
    insert into temperature (temp, time)
    values (#{temp,jdbcType=VARCHAR}, #{time,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.algridashboard.dashboard.model.Temperature" >
    insert into temperature
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="temp != null" >
        temp,
      </if>
      <if test="time != null" >
        time,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="temp != null" >
        #{temp,jdbcType=VARCHAR},
      </if>
      <if test="time != null" >
        #{time,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>